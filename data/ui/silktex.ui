<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.96.1 -->
<interface>
  <!-- interface-name silktex.ui -->
  <requires lib="gtk" version="4.18"/>
  <requires lib="libadwaita" version="1.6"/>
  <object class="AdwApplicationWindow" id="main-window">
    <property name="accessible-role">application</property>
    <property name="default-height">800</property>
    <property name="default-width">1200</property>
    <property name="show-menubar">True</property>
    <child>
      <object class="AdwHeaderBar" id="top-menubar">
        <property name="halign">baseline-fill</property>
        <property name="name">SilkTex</property>
        <property name="title-widget">
          <object class="AdwActionRow" id="title">
            <property name="accessible-role">application</property>
            <property name="title">SilkTex</property>
          </object>
        </property>
        <property name="valign">start</property>
        <child type="start">
          <object class="AdwButtonRow">
            <property name="end-icon-name">abrt</property>
            <property name="margin-end">20</property>
            <property name="margin-start">20</property>
          </object>
        </child>
        <style>
          <class name="mainbar"/>
        </style>
      </object>
    </child>
  </object>
  <object class="AdwPreferencesWindow">
    <child>
      <object class="AdwViewSwitcher">
        <property name="accessible-role">dialog</property>
        <property name="policy">wide</property>
        <child>
          <object class="AdwHeaderBar">
            <property name="valign">start</property>
            <child>
              <object class="GtkButton"/>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwOverlaySplitView"/>
        </child>
      </object>
    </child>
  </object>
</interface>
